<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="wrap_content" android:orientation="vertical"
	android:layout_width="fill_parent">
	<RelativeLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:gravity="center_horizontal">
		<TextView android:layout_width="wrap_content" android:id="@+id/viewItem1" android:layout_alignParentLeft="true"
			android:layout_height="wrap_content" android:text="@string/contact"></TextView>
		<EditText android:text="" android:id="@+id/editItem1"  android:layout_alignParentRight="true"
			android:layout_height="wrap_content" android:layout_width="160dip"></EditText>
	</RelativeLayout>
	<RelativeLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:gravity="center_horizontal">
		<TextView android:layout_width="wrap_content"  android:id="@+id/viewItem2"  android:layout_alignParentLeft="true"
			android:layout_height="wrap_content" android:text="@string/number"></TextView>
		<EditText android:text="" android:id="@+id/editItem2" android:inputType="number" android:layout_alignParentRight="true"
			android:layout_height="wrap_content" android:layout_width="160dip"></EditText>
	</RelativeLayout>
</LinearLayout>
